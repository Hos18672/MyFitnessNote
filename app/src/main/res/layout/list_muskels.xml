            <com.google.android.material.card.MaterialCardView
                android:id="@+id/shoulder_btn"
                android:layout_width="match_parent"
                android:layout_height="70dp"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginBottom="15dp"
                android:backgroundTint="@color/backgroundColorNav"
                android:contentDescription="@string/shoulder"
                android:paddingStart="20dp"
                android:text="@string/shoulder"
                android:textSize="20sp"
                android:transitionName="addTrainingCv"
                app:cardCornerRadius="10dp"
                app:rippleColor="#8ED1FF"
                tools:ignore="RtlHardcoded,RtlSymmetry">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_gravity="center"
